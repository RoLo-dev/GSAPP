@model List<User>
    @* brought list of users with same zip code as logged user *@

@{
    ViewData["Title"] = "Dashboard Page";
}

<h1>Dashboard</h1>

<a asp-action="RequestForm">Make a request!</a>

<h1>@ViewData["Title"]</h1>
<table>
    <thead>
        <tr>
            <th>FullName</th>
            <th>Location</th>    
            <th>Request Urgency</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            @if(user.RequestsCreated == null)
            {
            <tr>
                <td>@user.FirstName @user.LastName</td>
                <td>@user.City, @user.State</td>
                @* <td>@u</td> *@
                <td></td>
                <td><a asp-action="Detail" asp-route-Uid="@user.UserId">View</a></td>
            </tr>
            }
        }
    </tbody>
</table>
